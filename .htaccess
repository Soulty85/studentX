RewriteEngine On

#----------- Route globales -----------
RewriteRule ^login$ login.php
RewriteRule ^admin$ admin.php
RewriteRule ^home$ index.php

#----------- Route View -----------
RewriteRule ^listeUsers$ controller/user/UserMainController.php?page=etudiants
RewriteRule ^listeEval$ controller/evaluation/EvaluationMainController.php?page=evaluation

#----------- Route Controller -----------
RewriteRule ^userMainController$  controller/user/UserMainController.php
RewriteRule ^evalMainController$  controller/evaluation/EvaluationMainController.php

#----------- Route Model -----------
RewriteRule ^UserRepository$  model/UserRepository.php
